<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chowk.packag.com.onlinestorekotlin">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="chowk.packag.com.onlinestorekotlin.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="chowk.packag.com.onlinestorekotlin.SignUpLayout" />
        <activity android:name="chowk.packag.com.onlinestorekotlin.HomeScreen" />
        <activity android:name="chowk.packag.com.onlinestorekotlin.FetchProductsActivity" />
        <activity android:name="chowk.packag.com.onlinestorekotlin.CartProductsActivity" />
        <activity android:name="chowk.packag.com.onlinestorekotlin.FinalizeShoppingActivity" />
        <activity android:name="chowk.packag.com.onlinestorekotlin.ThankYouActivity"></activity>
    </application>

</manifest>